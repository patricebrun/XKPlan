{% extends 'layout.html.twig' %}

{% block body %}
    <h1>Modelesousperiode</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ modeleSousPeriode.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ modeleSousPeriode.nom }}</td>
            </tr>
            <tr>
                <th>Pourperiodeestivalle</th>
                <td>{% if modeleSousPeriode.pourPeriodeEstivalle %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Pourperiodehivernalle</th>
                <td>{% if modeleSousPeriode.pourPeriodeHivernalle %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Datetimemodification</th>
                <td>{% if modeleSousPeriode.dateTimeModification %}{{ modeleSousPeriode.dateTimeModification|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Datetimecreation</th>
                <td>{% if modeleSousPeriode.dateTimeCreation %}{{ modeleSousPeriode.dateTimeCreation|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('modelesousperiode_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('modelesousperiode_edit', { 'id': modeleSousPeriode.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
