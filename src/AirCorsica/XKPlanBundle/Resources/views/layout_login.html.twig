<!DOCTYPE html>
<html>

<head>
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">

    <title>{% block title %}XKPLAN - Air Corsica{% endblock %}</title>

    <meta name="keywords" content="XKPLAN - Air Corsica" />
    <meta name="description" content="XKPLAN - Air Corsica">
    <meta name="author" content="XKPLAN - Air Corsica">

    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- Favicon -->
    <link rel="shortcut icon" href="{{ asset('bundles/aircorsicaxkplan/assets/img/favicon.ico') }}">
    <link rel="apple-touch-icon" sizes="57x57" href="{{ asset('bundles/aircorsicaxkplan/assets/img/apple-icon-57x57.png') }}">
    <link rel="apple-touch-icon" sizes="60x60" href="{{ asset('bundles/aircorsicaxkplan/assets/img/apple-icon-60x60.png') }}">
    <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('bundles/aircorsicaxkplan/assets/img/apple-icon-72x72.png') }}">
    <link rel="apple-touch-icon" sizes="76x76" href="{{ asset('bundles/aircorsicaxkplan/assets/img/apple-icon-76x76.png') }}">
    <link rel="apple-touch-icon" sizes="114x114" href="{{ asset('bundles/aircorsicaxkplan/assets/img/apple-icon-114x114.png') }}">
    <link rel="apple-touch-icon" sizes="120x120" href="{{ asset('bundles/aircorsicaxkplan/assets/img/apple-icon-120x120.png') }}">
    <link rel="apple-touch-icon" sizes="144x144" href="{{ asset('bundles/aircorsicaxkplan/assets/img/apple-icon-144x144.png') }}">
    <link rel="apple-touch-icon" sizes="152x152" href="{{ asset('bundles/aircorsicaxkplan/assets/img/apple-icon-152x152.png') }}">
    <link rel="apple-touch-icon" sizes="180x180" href="{{ asset('bundles/aircorsicaxkplan/assets/img/apple-icon-180x180.png') }}">
    <link rel="icon" type="image/png" sizes="192x192" href="{{ asset('bundles/aircorsicaxkplan/assets/img/android-icon-192x192.png') }}">
    <link rel="icon" type="image/png" sizes="32x32" href="{{ asset('bundles/aircorsicaxkplan/assets/img/favicon-32x32.png') }}">
    <link rel="icon" type="image/png" sizes="96x96" href="{{ asset('bundles/aircorsicaxkplan/assets/img/favicon-96x96.png') }}">
    <link rel="icon" type="image/png" sizes="16x16" href="{{ asset('bundles/aircorsicaxkplan/assets/img/favicon-16x16.png') }}">
    <link rel="manifest" href="{{ asset('bundles/aircorsicaxkplan/assets/img/manifest.json') }}">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="{{ asset('bundles/aircorsicaxkplan/assets/img/ms-icon-144x144.png') }}">
    <meta name="theme-color" content="#ffffff">

    <!-- Font CSS (Via CDN) -->
    <link rel='stylesheet' type='text/css' href='http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700'>

    {% stylesheets
    'bundles/aircorsicaxkplan/assets/skin/default_skin/less/theme.less'
    'bundles/aircorsicaxkplan/assets/admin-tools/admin-forms/less/admin-forms.less'
    filter='less,cssrewrite'
    output='css/compiled/theme.css'
    %}

    <link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}" />

    {% endstylesheets %}

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->
</head>

<body class="external-page external-alt sb-l-c sb-r-c">

<!-- Start: Main -->
<div id="main" class="animated fadeIn">

    <!-- Start: Content-Wrapper -->
    <section id="content_wrapper">

        <!-- Begin: Content -->
        <div id="content" class="animated fadeIn">

            <div class="admin-form theme-info mw500" id="login">

                <!-- Login Logo -->
                <div class="row table-layout">
                    <a href="{{ path('air_corsica_xk_plan_homepage') }}" title="Retour à l'accueil">
                        <img src="{{ asset('bundles/aircorsicaxkplan/assets/img/logos/logo.png') }}" title="Logo Air Corsica" alt="Logo Air Corsica" class="center-block img-responsive mw500 br-home-logo" />
                    </a>
                </div>

                {% block fos_user_content %}
                {% endblock %}

                {#<!-- Registration Links -->#}
                {#<div class="login-links">#}
                    {#<p>#}
                        {#<a href="{{ path('air_corsica_xk_plan_forgot_test') }}" class="active" title="Sign In">Mot de passe oublié?</a>#}
                    {#</p>#}
                {#</div>#}

            </div>

        </div>
        <!-- End: Content -->

    </section>
    <!-- End: Content-Wrapper -->

</div>
<!-- End: Main -->


<!-- BEGIN: PAGE SCRIPTS -->

<!-- jQuery -->
{% javascripts
'bundles/aircorsicaxkplan/vendor/jquery/jquery-1.11.1.min.js'
'bundles/aircorsicaxkplan/vendor/jquery/jquery_ui/jquery-ui.min.js'
%}

<script type="text/javascript" src="{{ asset_url }}"></script>

{% endjavascripts %}

<!-- Theme Javascript -->
{% javascripts
'bundles/aircorsicaxkplan/assets/js/utility/utility.js'
'bundles/aircorsicaxkplan/assets/js/demo/demo.js'
'bundles/aircorsicaxkplan/assets/js/main.js'
%}

<script type="text/javascript" src="{{ asset_url }}"></script>

{% endjavascripts %}

<script type="text/javascript">
    jQuery(document).ready(function() {

        "use strict";

        // Init Theme Core
        Core.init();
    });
</script>
<!-- END: PAGE SCRIPTS -->

</body>

</html>
