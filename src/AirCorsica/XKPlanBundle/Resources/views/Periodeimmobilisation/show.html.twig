{% extends 'AirCorsicaXKPlanBundle::layout.html.twig' %}

{% block body %}
    <h1>Periodeimmobilisation</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ periodeImmobilisation.id }}</td>
            </tr>
            <tr>
                <th>Datetimemodification</th>
                <td>{% if periodeImmobilisation.dateTimeModification %}{{ periodeImmobilisation.dateTimeModification|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Datetimecreation</th>
                <td>{% if periodeImmobilisation.dateTimeCreation %}{{ periodeImmobilisation.dateTimeCreation|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('periodeimmobilisation_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('periodeimmobilisation_edit', { 'id': periodeImmobilisation.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
